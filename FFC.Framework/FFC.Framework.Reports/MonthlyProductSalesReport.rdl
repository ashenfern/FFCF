    <DataSet Name="MonthlySaleDataSet">
      <Query>
        <DataSourceName>FFCDataSource</DataSourceName>
        <CommandText>SELECT  B.BranchID,P.ProductID,P.ProductName,DATEPART(MONTH, TransactionDate) as Month,DATEPART(Day, TransactionDate) as Day, SUM(TD.Quantity) as Count FROM Transactions T
INNER JOIN TransactionDetails TD on T.TransactionID = TD.TransactionID
INNER JOIN Products P ON TD.ProductID = P.ProductID
INNER JOIN Categories C ON P.CategoryID = C.CategoryID
INNER JOIN Branches B ON B.BranchID = T.BranchID
Group BY DATEPART(MONTH, TransactionDate),DATEPART(Day, TransactionDate), P.ProductID,P.ProductName, B.BranchID
Order By B.BranchID,P.ProductID, P.ProductName,DATEPART(MONTH, TransactionDate),DATEPART(Day, TransactionDate) </CommandText>
      </Query>
      <Fields>
        <Field Name="BranchID">
          <DataField>BranchID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductId">
          <DataField>ProductID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Day">
          <DataField>Day</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!BranchID.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!BranchID.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
