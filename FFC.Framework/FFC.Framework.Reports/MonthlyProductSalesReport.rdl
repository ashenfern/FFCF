    <DataSet Name="BranchesDataSet">
      <Query>
        <DataSourceName>FFCDataSource</DataSourceName>
        <CommandText>SELECT        BranchID, BranchName, BranchAddress, StartTime, EndTime
FROM            Branches</CommandText>
      </Query>
